{
  "name": "typeserve",
  "version": "1.0.0",
  "description": "TypeScript-first tool that generates live mock APIs from TypeScript types",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "typeserve": "node packages/cli/dist/index.js",
    "typeserve:dev": "npx tsx packages/cli/src/index.ts"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/emmanueltaiwo/typeserve.git"
  },
  "license": "AGPL-3.0",
  "keywords": [
    "typescript",
    "mock",
    "api",
    "mock-api",
    "types",
    "development",
    "cli"
  ]
}
