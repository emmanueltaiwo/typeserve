{
  "name": "@typeserve/core",
  "version": "2.0.4",
  "description": "Core library for TypeServe - TypeScript mock API generator",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "typescript",
    "mock",
    "api",
    "types"
  ],
  "author": "Emmanuel Taiwo <devemmanuel1@gmail.com>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/emmanueltaiwo/typeserve.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "chokidar": "^4.0.3",
    "express": "^5.2.1",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
